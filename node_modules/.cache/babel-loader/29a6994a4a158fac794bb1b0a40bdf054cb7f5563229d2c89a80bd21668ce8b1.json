{"ast":null,"code":"import axios from 'axios';\nimport EditUser from '@/components/Edit.vue';\nexport default {\n  data() {\n    return {\n      searchUser: '',\n      interns: [],\n      filteredInterns: [],\n      userList: []\n    };\n  },\n  mounted() {\n    this.fetchUserList();\n  },\n  computed: {\n    filteredUserList() {\n      if (this.searchUser.trim() === '') {\n        return this.userList;\n      }\n      const searchTerm = this.searchUser.toLowerCase();\n      return this.userList.filter(user => {\n        const fullName = `${user.first_name} ${user.last_name}`.toLowerCase();\n        return fullName.includes(searchTerm);\n      });\n    }\n  },\n  methods: {\n    async search() {\n      try {\n        if (this.searchUser.trim() === '') {\n          this.filteredInterns = [];\n          return;\n        }\n        const response = await axios.get('https://reqres.in/api/users', {\n          params: {\n            q: this.searchUser\n          }\n        });\n        this.interns = response.data.data;\n        this.filterInterns();\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    filterInterns() {\n      this.filteredInterns = this.interns.filter(intern => {\n        const fullName = `${intern.first_name} ${intern.last_name}`.toLowerCase();\n        return fullName.includes(this.searchUser.toLowerCase()) && this.userList.some(user => user.id === intern.id);\n      });\n    },\n    fetchUserList() {\n      axios.get('https://reqres.in/api/users').then(response => {\n        this.userList = response.data.data.map(user => ({\n          id: user.id,\n          fullName: `${user.first_name} ${user.last_name}`,\n          avatar: user.avatar\n        }));\n      }).catch(error => {\n        console.error('Failed to fetch user list:', error);\n      });\n    },\n    handleUserDeleted(userId) {\n      const userIndex = this.userList.findIndex(user => user.id === userId);\n      if (userIndex !== -1) {\n        this.userList.splice(userIndex, 1);\n      }\n    },\n    handleUserUpdated(updatedUser) {\n      const userIndex = this.userList.findIndex(user => user.id === updatedUser.id);\n      if (userIndex !== -1) {\n        this.userList[userIndex].fullName = `${updatedUser.first_name} ${updatedUser.last_name}`;\n      }\n    }\n  },\n  components: {\n    EditUser\n  }\n};","map":{"version":3,"names":["axios","EditUser","data","searchUser","interns","filteredInterns","userList","mounted","fetchUserList","computed","filteredUserList","trim","searchTerm","toLowerCase","filter","user","fullName","first_name","last_name","includes","methods","search","response","get","params","q","filterInterns","error","console","intern","some","id","then","map","avatar","catch","handleUserDeleted","userId","userIndex","findIndex","splice","handleUserUpdated","updatedUser","components"],"sources":["C:\\Users\\User\\Desktop\\Client\\vue-starter-project\\src\\components\\Hello.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <div class=\"search-container\">\r\n        <input type=\"text\" placeholder=\"Search...\" v-model=\"searchUser\" @input=\"search\" />\r\n        <button class=\"add-user-btn\">Add User</button>\r\n      </div>\r\n      <ul>\r\n        <li v-for=\"intern in filteredInterns\" :key=\"intern.id\">\r\n          {{ intern.first_name }} {{ intern.last_name }}\r\n        </li>\r\n      </ul>\r\n  \r\n      <div class=\"user-list\">\r\n        <div v-for=\"user in userList\" :key=\"user.id\" class=\"user-item\">\r\n          <div class=\"user-avatar\">\r\n            <img :src=\"user.avatar\" alt=\"User Avatar\" />\r\n          </div>\r\n          <div class=\"user-details\">\r\n            <div class=\"user-name\">{{ user.fullName }}</div>\r\n            <div class=\"user-actions\">\r\n              <router-link :to=\"{ name: 'edit-user', params: { id: user.id } }\">Edit</router-link>\r\n              <button @click=\"handleUserDeleted(user.id)\">Delete</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  import EditUser from '@/components/Edit.vue';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        searchUser: '',\r\n        interns: [],\r\n        filteredInterns: [],\r\n        userList: [],\r\n      };\r\n    },\r\n  \r\n    mounted() {\r\n      this.fetchUserList();\r\n    },\r\n  \r\n    computed: {\r\n      filteredUserList() {\r\n        if (this.searchUser.trim() === '') {\r\n          return this.userList;\r\n        }\r\n  \r\n        const searchTerm = this.searchUser.toLowerCase();\r\n        return this.userList.filter((user) => {\r\n          const fullName = `${user.first_name} ${user.last_name}`.toLowerCase();\r\n          return fullName.includes(searchTerm);\r\n        });\r\n      },\r\n    },\r\n  \r\n    methods: {\r\n      async search() {\r\n        try {\r\n          if (this.searchUser.trim() === '') {\r\n            this.filteredInterns = [];\r\n            return;\r\n          }\r\n  \r\n          const response = await axios.get('https://reqres.in/api/users', {\r\n            params: {\r\n              q: this.searchUser,\r\n            },\r\n          });\r\n  \r\n          this.interns = response.data.data;\r\n          this.filterInterns();\r\n        } catch (error) {\r\n          console.error(error);\r\n        }\r\n      },\r\n  \r\n      filterInterns() {\r\n        this.filteredInterns = this.interns.filter((intern) => {\r\n          const fullName = `${intern.first_name} ${intern.last_name}`.toLowerCase();\r\n          return (\r\n            fullName.includes(this.searchUser.toLowerCase()) &&\r\n            this.userList.some((user) => user.id === intern.id)\r\n          );\r\n        });\r\n      },\r\n  \r\n      fetchUserList() {\r\n        axios\r\n          .get('https://reqres.in/api/users')\r\n          .then((response) => {\r\n            this.userList = response.data.data.map((user) => ({\r\n              id: user.id,\r\n              fullName: `${user.first_name} ${user.last_name}`,\r\n              avatar: user.avatar,\r\n            }));\r\n          })\r\n          .catch((error) => {\r\n            console.error('Failed to fetch user list:', error);\r\n          });\r\n      },\r\n  \r\n      handleUserDeleted(userId) {\r\n        const userIndex = this.userList.findIndex((user) => user.id === userId);\r\n        if (userIndex !== -1) {\r\n          this.userList.splice(userIndex, 1);\r\n        }\r\n      },\r\n  \r\n      handleUserUpdated(updatedUser) {\r\n        const userIndex = this.userList.findIndex((user) => user.id === updatedUser.id);\r\n        if (userIndex !== -1) {\r\n          this.userList[userIndex].fullName = `${updatedUser.first_name} ${updatedUser.last_name}`;\r\n        }\r\n      },\r\n    },\r\n  \r\n    components: {\r\n      EditUser,\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  /* Styles for the component */\r\n  </style>\r\n  "],"mappings":"AA8BE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,uBAAuB;AAE5C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE,EAAE;MACXC,eAAe,EAAE,EAAE;MACnBC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;EACtB,CAAC;EAEDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB,IAAI,IAAI,CAACP,UAAU,CAACQ,IAAI,CAAC,MAAM,EAAE,EAAE;QACjC,OAAO,IAAI,CAACL,QAAQ;MACtB;MAEA,MAAMM,UAAS,GAAI,IAAI,CAACT,UAAU,CAACU,WAAW,CAAC,CAAC;MAChD,OAAO,IAAI,CAACP,QAAQ,CAACQ,MAAM,CAAEC,IAAI,IAAK;QACpC,MAAMC,QAAO,GAAK,GAAED,IAAI,CAACE,UAAU,IAAIF,IAAI,CAACG,SAAU,EAAC,CAACL,WAAW,CAAC,CAAC;QACrE,OAAOG,QAAQ,CAACG,QAAQ,CAACP,UAAU,CAAC;MACtC,CAAC,CAAC;IACJ;EACF,CAAC;EAEDQ,OAAO,EAAE;IACP,MAAMC,MAAMA,CAAA,EAAG;MACb,IAAI;QACF,IAAI,IAAI,CAAClB,UAAU,CAACQ,IAAI,CAAC,MAAM,EAAE,EAAE;UACjC,IAAI,CAACN,eAAc,GAAI,EAAE;UACzB;QACF;QAEA,MAAMiB,QAAO,GAAI,MAAMtB,KAAK,CAACuB,GAAG,CAAC,6BAA6B,EAAE;UAC9DC,MAAM,EAAE;YACNC,CAAC,EAAE,IAAI,CAACtB;UACV;QACF,CAAC,CAAC;QAEF