ton>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\n\r\nexport default {\r\n  computed: {\r\n    user() {\r\n      return this.getUserById(this.userId);\r\n    },\r\n    userId() {\r\n      return this.$route.params.id;\r\n    },\r\n    ...mapGetters(['getUserById'])\r\n  },\r\n  mounted() {\r\n    if (!this.user) {\r\n      this.$store.dispatch('fetchUserList', this.userId);\r\n    }\r\n  },\r\n  methods: {\r\n    async saveChanges() {\r\n      try {\r\n        await this.$store.dispatch('updateUser', this.user);\r\n        this.$router.push({ name: 'home' });\r\n      } catch (error) {\r\n        console.error('Failed to update user:', error);\r\n      }\r\n    },\r\n    async deleteUser() {\r\n      try {\r\n        await this.$store.dispatch('deleteUser', this.user.id);\r\n        this.$router.push({ name: 'home' });\r\n      } catch (error) {\r\n        console.error('Failed to delete user:', error);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":";AA0BA,SAASA,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,QAAQ,EAAE;IACRC,IAAIA,CAAA,EAAG;MACL,OAAO,IAAI,CAACC,WAAW,CAAC,IAAI,CAACC,MAAM,CAAC;IACtC,CAAC;IACDA,MAAMA,CAAA,EAAG;MACP,OAAO,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE;IAC9B,CAAC;IACD,GAAGP,UAAU,CAAC,CAAC,aAAa,CAAC;EAC/B,CAAC;EACDQ,OAAOA,CAAA,EAAG;IACR,IAAI,CAAC,IAAI,CAACN,IAAI,EAAE;MACd,IAAI,CAACO,MAAM,CAACC,QAAQ,CAAC,eAAe,EAAE,IAAI,CAACN,MAAM,CAAC;IACpD;EACF,CAAC;EACDO,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI;QACF,MAAM,IAAI,CAACH,MAAM,CAACC,QAAQ,CAAC,YAAY,EAAE,IAAI,CAACR,IAAI,CAAC;QACnD,IAAI,CAACW,OAAO,CAACC,IAAI,CAAC;UAAEC,IAAI,EAAE;QAAO,CAAC,CAAC;MACrC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;MAChD;IACF,CAAC;IACD,MAAME,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAM,IAAI,CAACT,MAAM,CAACC,QAAQ,CAAC,YAAY,EAAE,IAAI,CAACR,IAAI,CAACK,EAAE,CAAC;QACtD,IAAI,CAACM,OAAO,CAACC,IAAI,CAAC;UAAEC,IAAI,EAAE;QAAO,CAAC,CAAC;MACrC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,wBAAw