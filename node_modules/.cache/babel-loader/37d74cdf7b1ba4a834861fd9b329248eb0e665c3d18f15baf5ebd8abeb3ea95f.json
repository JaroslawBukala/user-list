rection: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  background-color: #f5f5f5;\r\n  padding: 20px;\r\n}\r\n\r\n.add-user-title {\r\n  margin-bottom: 10px;\r\n  font-weight: 400;\r\n  font-size: 18px;\r\n  color: #000;\r\n}\r\n\r\n.add-user-section {\r\n  display: flex;\r\n  flex-direction: row;\r\n  align-items: flex-start;\r\n  gap: 20px;\r\n}\r\n\r\n.avatar-container {\r\n  position: relative;\r\n}\r\n\r\n.avatar-placeholder {\r\n  position: relative;\r\n  width: 150px;\r\n  height: 150px;\r\n  border: 2px dashed #ccc;\r\n  border-radius: 50%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  cursor: pointer;\r\n}\r\n\r\n.avatar-image {\r\n  width: 100%;\r\n  height: 100%;\r\n  border-radius: 50%;\r\n  object-fit: cover;\r\n}\r\n\r\n.select-image-button {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  opacity: 0;\r\n  cursor: pointer;\r\n}\r\n\r\n.change-photo-button {\r\n  position: absolute;\r\n  bottom: 10px;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n  background-color: transparent;\r\n  border: none;\r\n  color: #000;\r\n  font-size: 14px;\r\n  cursor: pointer;\r\n}\r\n\r\n.name-section {\r\n  flex: 1;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.form-field {\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.form-field label {\r\n  margin-bottom: 5px;\r\n  font-weight: 400;\r\n  font-size: 14px;\r\n  color: #000;\r\n}\r\n\r\n.form-field input {\r\n  width: 100%;\r\n  padding: 8px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 3px;\r\n}\r\n\r\n.update-button {\r\n  background-color: green;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 3px;\r\n  padding: 8px 16px;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"mappings":";AA2BA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,MAAMA,CAAA,EAAG;MACP,OAAO,IAAI,CAACC,MAA