tch('fetchUserList');\r\n    },\r\n  \r\n    methods: {\r\n      searchUsers() {\r\n        this.$store.commit('filterUserList', this.searchQuery);\r\n      },\r\n  \r\n      deleteUser(userId) {\r\n        this.$store.dispatch('deleteUser', userId);\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  .search-container {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  .search-container input[type=\"text\"] {\r\n    margin-right: 10px;\r\n    padding: 5px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 4px;\r\n  }\r\n  \r\n  .add-user-btn {\r\n    background-color: green;\r\n    color: white;\r\n    border: none;\r\n    padding: 5px 10px;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .user-list {\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n  \r\n  .user-item {\r\n    display: flex;\r\n    align-items: center;\r\n    padding: 10px;\r\n    border-bottom: 1px solid #ccc;\r\n  }\r\n  \r\n  .user-avatar img {\r\n    width: 50px;\r\n    height: 50px;\r\n    border-radius: 50%;\r\n    object-fit: cover;\r\n    margin-right: 10px;\r\n  }\r\n  \r\n  .user-name {\r\n    font-weight: bold;\r\n  }\r\n  \r\n  .user-actions button {\r\n    margin-left: 10px;\r\n    padding: 5px 10px;\r\n    background-color: red;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n  }\r\n  </style>\r\n  "],"mappings":";;EAuIWA,KAAK,EAAC;AAAkB;gCAE3BC,mBAAA,CAA8C;EAAtCD,KAAK,EAAC;AAAc,GAAC,UAAQ;;EAI9BA,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAa;;;EAGnBA,KAAK,EAAC;AAAc;;EAClBA,KAAK,EAAC;AAAW;;EACjBA,KAAK,EAAC;AAAc;;;;uBAbnCE,mBAAA,CAqBM,cApBJD,mBAAA,CAGM,OAHNE,UAGM,G,gBAFJF,mBAAA,CAAwF;IAAjFG,IAAI,EAAC,MAAM;IAACC,WAAW,EAAC,WAAW;+DAAUC,KAAA,CAAAC,WAAW,GAAAC,MAAA;IAAGC,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,WAAA,IAAAD,QAAA,CAAAC,WAAA,IAAAF,IAAA,CAAW;iEAAhCL,KAAA,CAAAC,WAAW,E,GAC/DO,UAA8C,C,GAEhDb,mBAAA,CAeK,c,kBAdHC,mBAAA,CAaKa,SAAA,QAAA