n>\r\n      </div>\r\n      <ul>\r\n        <li v-for=\"intern in filteredInterns\" :key=\"intern.id\">\r\n          {{ intern.first_name }} {{ intern.last_name }}\r\n        </li>\r\n      </ul>\r\n  \r\n      <div class=\"user-list\">\r\n        <div v-for=\"user in userList\" :key=\"user.id\" class=\"user-item\">\r\n          <div class=\"user-avatar\">\r\n            <img :src=\"user.avatar\" alt=\"User Avatar\">\r\n          </div>\r\n          <div class=\"user-details\">\r\n            <div class=\"user-name\">{{ user.fullName }}</div>\r\n            <div class=\"user-actions\">\r\n              <router-link :to=\"{ name: 'edit-user', params: { id: user.id } }\">Edit</router-link>\r\n              <button @click=\"handleUserDeleted(user.id)\">Delete</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n  \r\n      <router-link to=\"/\">Home</router-link>\r\n      <router-link to=\"/about\">About</router-link>\r\n  \r\n      <div v-if=\"showEditModal && selectedUserId\">\r\n        <edit-user\r\n          :user=\"selectedUser\"\r\n          @user-updated=\"handleUserUpdated\"\r\n          @user-deleted=\"handleUserDeleted\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\nimport { computed, ref } from 'vue';\r\nimport { useStore } from 'vuex';\r\nimport EditUser from '@/components/Edit.vue';\r\n\r\nexport default {\r\n  components: {\r\n    EditUser,\r\n  },\r\n  setup() {\r\n    const store = useStore();\r\n    const searchUser = ref('');\r\n    const showEditModal = ref(false);\r\n    const selectedUserId = ref(null);\r\n\r\n    const userList = computed(() => {\r\n      return store.state.userList.filter((user) =>\r\n        user.fullName.toLowerCase().includes(searchUser.value.toLowerCase())\r\n      );\r\n    });\r\n\r\n    const handleUserUpdated = (updatedUser) => {\r\n      store.dispatch('updateUser', updatedUser);\r\n      showEditModal.value = false;\r\n    };\r\n\r\n    const handleUserDeleted = (userId) => {\r\n      store.dispatch('deleteUser', userId);\r\n      showEditModal.value = false;\r\n    };\r\n\r\n    return {\r\n      searchUser,\r\n      userList,\r\n      showEditModal,\r\n      selectedUserId,\r\n      selectedUser: computed(() => {\r\n        return store.state.userList.find((user) => user.id === selectedUserId.value);\r\n      }),\r\n      search,\r\n      handleUserUpdated,\r\n      handleUserDeleted,\r\n    };\r\n\r\n    function search() {\r\n      // Implement your search logic here\r\n      // You can use the searchUser value to filter the user list\r\n      // You can access the user list as store.state.userList\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n  \r\n  <style>\r\n  /* Add your custom styles here */\r\n  .search-container {\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  .add-user-btn {\r\n    margin-left: 10px;\r\n  }\r\n  \r\n  .user-list {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n  }\r\n  \r\n  .user-item {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-bottom: 10px;\r\n    padding: 10px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 4px;\r\n  }\r\n  \r\n  .user-avatar {\r\n    width: 50px;\r\n    height: 50px;\r\n    margin-right: 10px;\r\n  }\r\n  \r\n  .user-avatar img {\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: cover;\r\n    border-radius: 50%;\r\n  }\r\n  \r\n  .user-details {\r\n    flex: 1;\r\n  }\r\n  \r\n  .user-name {\r\n    font-weight: bold;\r\n  }\r\n  \r\n  .user-actions button {\r\n    margin-right: 5px;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAyCA,SAASA,QAAQ,EAAEC,GAAE,QAAS,KAAK;AACnC,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,QAAO,MAAO,uBAAuB;AAE5C,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIJ,QAAQ,CAAC,CAAC;IACxB,MAAMK,UAAS,GAAIN,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMO,aAAY,GAAIP,GAAG,CAAC,KAAK,CAAC;IAChC,MAAMQ,cAAa,GAAIR,GAAG,CAAC,IAAI,CAAC;IAEhC,MAAMS,QAAO,GAAIV,QAAQ,CAAC,MAAM;MAC9B,OAAOM,KAAK,CAACK,KAAK,CAACD,QAAQ,C