atar\" :src=\"user.avatar\" class=\"avatar-image\" alt=\"Avatar\">\r\n          <span v-else class=\"camera-icon\"></span>\r\n        </div>\r\n      </div>\r\n      <button class=\"change-photo-button\" @click=\"openFileDialog\">Change Photo</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      user: {},\r\n    };\r\n  },\r\n  computed: {\r\n    userId() {\r\n      return this.$route.params.id;\r\n    },\r\n  },\r\n  mounted() {\r\n    // Fetch the user data based on the provided ID\r\n    this.user = this.$store.state.filteredUserList[this.userId - 1];\r\n  },\r\n  methods: {\r\n    saveChanges() {\r\n      try {\r\n        this.$store.dispatch('updateUser', this.user);\r\n        this.$router.push({ name: 'home' });\r\n      } catch (error) {\r\n        console.error('Failed to update user:', error);\r\n      }\r\n    },\r\n    deleteUser() {\r\n      try {\r\n        this.$store.dispatch('deleteUser', this.user.id);\r\n        this.$router.push({ name: 'home' });\r\n      } catch (error) {\r\n        console.error('Failed to delete user:', error);\r\n      }\r\n    },\r\n    openFileDialog() {\r\n      // Add the logic for opening file dialog if needed\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n\r\n\r\n<style scoped>\r\n\r\n\r\n\r\n.delete-button {\r\n  font-family: Arial, Helvetica, sans-serif;\r\n  background-color: #f44336;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 3px;\r\n  padding: 0.7em 1em;\r\n  cursor: pointer;\r\n  margin-top: 2em;\r\n}\r\n\r\n\r\n \r\n  h2 {\r\n    font-family: Arial, Helvetica, sans-serif;\r\n    margin-left: auto;\r\n    align-items: flex-start;\r\n    margin-top: 2em;\r\n  }\r\n\r\n  label {\r\n    display: inline block;\r\n    width: auto\r\n  }\r\n\r\n  input {\r\n    display: flex;\r\n    width: 99%; /* Adjust the width as needed */\r\n      \r\n  }\r\n\r\n  form input {\r\n    flex: 1;\r\n    padding: 0.5em;\r\n    border: 1px solid #ccc;\