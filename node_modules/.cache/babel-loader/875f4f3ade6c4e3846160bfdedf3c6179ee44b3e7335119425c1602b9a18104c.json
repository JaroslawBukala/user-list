h === 0) {\r\n          store.dispatch('fetchUserList');\r\n        }\r\n      };\r\n  \r\n      onMounted(fetchUserList);\r\n  \r\n      return {\r\n        searchUser,\r\n        filteredUserList,\r\n        showEditModal,\r\n        selectedUserId,\r\n        editUser,\r\n        deleteUser,\r\n        handleUserUpdated,\r\n        handleUserDeleted,\r\n      };\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  .search-container {\r\n    display: flex;\r\n    align-items: center;\r\n  }\r\n  \r\n  input[type=\"text\"] {\r\n    margin-right: 10px;\r\n  }\r\n  \r\n  .add-user-btn {\r\n    background-color: green;\r\n    color: white;\r\n    border: none;\r\n    padding: 5px 10px;\r\n  }\r\n  \r\n  ul {\r\n    list-style-type: none;\r\n    padding: 0;\r\n  }\r\n  \r\n  li {\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  li button {\r\n    margin-left: 10px;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAyBE,SAASA,QAAQ,EAAEC,SAAS,EAAEC,GAAE,QAAS,KAAK;AAC9C,SAASC,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIF,QAAQ,CAAC,CAAC;IACxB,MAAMG,UAAS,GAAIJ,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMK,aAAY,GAAIL,GAAG,CAAC,KAAK,CAAC;IAChC,MAAMM,cAAa,GAAIN,GAAG,CAAC,IAAI,CAAC;IAEhC,MAAMO,gBAAe,GAAIT,QAAQ,CAAC,MAAM;MACtC,MAAMU,UAAS,GAAIJ,UAAU,CAACK,KAAK,CAACC,WAAW,CAAC,CAAC;MACjD,OAAOP,KAAK,CAACQ,KAAK,CAACC,QAAQ,CAACC,MAAM,CAC/BC,IAAI,IACHA,IAAI,CAACC,QAAQ,CAACL,WAAW,CAAC,CAAC,CAACM,QAAQ,CAACR,UAAU,KAC/CM,IAAI,CAACG,KAAK,CAACP,WAAW,CAAC,CAAC,CAACM,QAAQ,CAACR,UAAU,CAChD,CAAC;IACH,CAAC,CAAC;IAEF,MAAMU,QAAO,GAAKJ,IAAI,IAAK;MACzBR,cAAc,CAACG,KAAI,GAAIK,IAAI,CAACK,EAAE;MAC9Bd,aAAa,CAACI,KAAI,GAAI,IAAI;IAC5B,CAAC;IAED,MAAMW,UAAS,GAAKC,MAAM,IAAK;MAC7BlB,KAAK,CAACmB,QAAQ,CAAC,YAAY,EAAED,MAAM,CAAC;IACtC,CAAC;IAED,MAAME,iBAAgB,GAAKC,WAAW,IAAK;MACzCrB,KAAK,CAACmB,QAAQ,CAAC,YAAY,EAAEE,WAAW,CAAC;MACzCnB,aAAa,CAACI,KAAI,GAAI,KAAK;IAC7B,CAAC;IAED,MAAMgB,iBAAgB,GAAKJ,MAAM,IAAK;MACpClB,KAAK,CAACmB,QAAQ,CAAC,YAAY,EAAED,MAAM,CAAC;MACpChB,aAAa,CAACI,KAAI,GAAI,KAAK;IAC7B,CAAC;IAED,MAAMiB,aA