     <img :src=\"user.avatar ? user.avatar : avatarPreview\" class=\"avatar-image\" alt=\"Avatar\">\r\n          <!-- <span v-else class=\"camera-icon\"></span> -->\r\n          <input type=\"file\" ref=\"fileInput\" style=\"display: none\" @change=\"handleFileChange\">\r\n        </div>\r\n      </div>\r\n      <button class=\"change-photo-button\" @click=\"openFileDialog\">Change Photo</button>\r\n    </div>\r\n    </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { mapGetters } from 'vuex';\r\n\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n        // user: {\r\n        //   first_name:'',\r\n        //   last_name: '',\r\n        //   avatar: ''\r\n        // },\r\n        avatar : '',\r\n        avatarPreview : null\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    user() {\r\n        console.log(this.$store.state.filteredUserList[this.$route.params.id - 1] + 'USER WITHOUT AVATAR')\r\n        return this.$store.state.filteredUserList[this.$route.params.id - 1];\r\n      },\r\n  },\r\n  methods: {\r\n\r\n    async saveChanges() {\r\n  const updatedUser = {\r\n    id: this.user.id,\r\n    first_name: this.user.first_name,\r\n    last_name: this.user.last_name,\r\n    avatar: this.user.avatar,\r\n  };\r\n  try {\r\n    await this.$store.dispatch('updateUser', updatedUser);\r\n    this.$store.dispatch('filterUserList', ' ');\r\n    this.$router.push({ name: 'home' });\r\n  } catch (error) {\r\n    console.error('Failed to update user:', error);\r\n  }\r\n},\r\n\r\n\r\nopenFileDialog() {\r\n      this.$refs.fileInput.click();\r\n    },\r\n    handleFileChange(event) {\r\n      const file = event.target.files[0];\r\n      if (file) {\r\n        console.log(file)\r\n        this.avatarPreview = URL.createObjectURL(file);\r\n        // You can also upload the file to the server here if needed\r\n        this.user.avatar = URL.createObjectURL(file);\r\n      } else {\r\n        console.log(this.avatarImage)\r\n        this.avatarPreview =  URL.createObjectURL(require('../assets/Avatar.png')) ;\r\n        this.avatar = URL.createObjectURL(require('../assets/Avatar.png')) ;\r\n      }\r\n    },\r\n    // this.$store.dispatch('deleteUser', userId);\r\n\r\n  deleteUser() {\r\n    this.$store\r\n      .dispatch('deleteUser', this.user.id)\r\n      .then(() => {\r\n        this.$store.dispatch('filterUserList', '');\r\n        this.$store.dispatch('clearSearchQuery'); // Dispatch the clearSearchQuery action\r\n        this.$router.push({ name: 'home' });\r\n      })\r\n      .catch(error => {\r\n        console.error('Failed to delete user:', error);\r\n      });\r\n  },\r\n\r\n\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n \r\n  h2 {\r\n    font-family: Arial, Helvetica, sans-serif;\r\n    margin-left: auto;\r\n    align-items: flex-start;\r\n    margin-top: 2em;\r\n  }\r\n\r\n  label {\r\n    display: inline block;\r\n    width: auto\r\n  }\r\n\r\n  input {\r\n    display: flex;\r\n    width: 99%; /* Adjust the width as needed */\r\n      \r\n  }\r\n\r\n  form input {\r\n    font-family: Arial, Helvetica, sans-serif;\r\n    flex: 1;\r\n    padding: 0.4em;\r\n    border: 1px solid #ccc;\r\n    border-radius: 3px;\r\n    line-height: 1.4;\r\n    font-size: 0.8em;\r\n\r\n  }\r\n.flex {\r\n  display: flex;\r\n  margin-right: 0.7em;\r\n}\r\n.margin {\r\n  padding: 1.1em;\r\n}\r\n\r\n  .edit-user-title {\r\n    align-items: flex-start; \r\n    display: flex;\r\n    margin-left: 0;\r\n    top: 0.2em;\r\n    right: 0em;\r\n    bottom: 0em;\r\n  }\r\n\r\n  .title-layout {\r\n    display: flex;\r\n    align-items: flex-start; \r\n    background-color: #f5f5f5;\r\n    padding: 0em 4em 2em 4em;\r\n    margin: 0em 4em 0em 4em;\r\n  }\r\n\r\n  .edit-user-container {\r\n    min-height: 100vh;\r\n    justify-content: center;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: flex-start; \r\n    background-color: #f5f5f5;\r\n    padding: 0em 4em 2em 4em;\r\n    margin: 0em 4em 0e