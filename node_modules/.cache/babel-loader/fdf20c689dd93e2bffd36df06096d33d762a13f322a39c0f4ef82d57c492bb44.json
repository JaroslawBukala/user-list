bottom: 0.01em;\r\n    margin-top: 8em;\r\n    float: right;\r\n  }\r\n\r\n  .avatar-container {\r\n    margin-top: 2.1em;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n\r\n  .avatar-placeholder {\r\n    display: flex;\r\n    background-image: url('../assets/Avatar.png');\r\n    width: 7em;\r\n    height: 7em;\r\n    background-color: #f5f5f5;\r\n    border-radius: 50%;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    cursor: pointer;\r\n    border: 1px solid #ccc;\r\n    background-size: cover;\r\n  }\r\n\r\n  .avatar-image {\r\n    width: 9.375em;\r\n    height: 9.375em;\r\n    border-radius: 50%;\r\n    object-fit: cover;\r\n  }\r\n\r\n  .camera-icon {\r\n    width: 1.875em;\r\n    height: 1.875em;\r\n    /* background-image: url('path/to/camera-icon.png'); */\r\n    background-size: cover;\r\n  }\r\n\r\n  .change-photo-button {\r\n    font-family: Arial, Helvetica, sans-serif;\r\n    font-size: 0.7em;\r\n    background-color: transparent;\r\n    border: 1px solid #ccc;\r\n    border-radius: 3px;\r\n    color: #000;\r\n    cursor: pointer;\r\n    width: 100%;\r\n    margin-top: 5.55em;\r\n    margin-bottom: 0.1em;\r\n    padding-top: 0.8em;\r\n    padding-bottom: 0.5em;\r\n  }\r\n\r\n  /* Media Query for Tablets and iPads */\r\n  @media (max-width: 1024px) {\r\n\r\n\r\n    .change-photo-button {\r\n      margin-top: 4em;\r\n    }\r\n\r\n    .margin {\r\n    display: none;\r\n\r\n  }\r\n  .flex {\r\n    display: inline-block;\r\n    flex-direction: column;\r\n    margin-right: 0.7em;\r\n  }\r\n\r\n  form {\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n\r\n    .form-field-inline {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin-bottom: 15px;\r\n    width: 100%;\r\n  }\r\n\r\n  .form-field-inline label {\r\n    flex: 0 0 30%;\r\n    margin-right: 1em;\r\n    margin-top: 0em;\r\n  }\r\n\r\n  .form-field-inline input {\r\n    flex: 1;\r\n    padding: 0.5em;\r\n    border: 1px solid #ccc;\r\n    border-radius: 3px;\r\n  }\r\n\r\n    .edit-user-container {\r\n      display: flex;\r\n      flex-direction: row;\r\n    }\r\n\r\n\r\n    .add-avatar-section {\r\n      display: flex;\r\n      flex-direction: column;\r\n    }\r\n    .form-field-inline {\r\n    width: 100%; \r\n    margin-top: 0em;\r\n  }\r\n\r\n  .update-button {\r\n    margin-bottom: 0.01em;\r\n    margin-top: 0.7em;\r\n  }\r\n\r\n  .delete-button {\r\n    margin-bottom: 0.01em;\r\n    margin-top: 0.7em;\r\n  }\r\n\r\n}\r\n\r\n  @media (max-width: 500px) {\r\n\r\n    .delete-button {\r\n      white-space: nowrap;\r\n\r\n    }\r\n    .title-layout {\r\n      justify-content: center;\r\n  }\r\n    .edit-user-container {\r\n      display: flex;\r\n      flex-direction: column;\r\n      margin-left: 0em;\r\n    }\r\n\r\n    .edit-user-section {\r\n      width: 130px;\r\n      flex: 0;\r\n      display: flex;\r\n      flex-direction: column;\r\n      margin-left: 0em;\r\n    }\r\n    .add-avatar-section {\r\n      width: 145px;\r\n      display: flex;\r\n      flex-direction: column;\r\n      margin-top: 1em;\r\n      margin-left: 0em;\r\n    }\r\n    .form-field-inline {\r\n    width: 100%; /* Adjust the width as needed */\r\n  }\r\n\r\n  }\r\n\r\n</style>\r\n"],"mappings":";;;;;gEAEIA,mBAAA,CAEM;EAFDC,KAAK,EAAC;AAAc,I,aACzBD,mBAAA,CAA0C;EAAtCC,KAAK,EAAC;AAAiB,GAAC,WAAS,E;;EAEhCA,KAAK,EAAC;AAAqB;;EAEzBA,KAAK,EAAC;AAAmB;;EAEvBA,KAAK,EAAC;AAAM;;EACVA,KAAK,EAAC;AAAmB;gEAC9BD,mBAAA,CAA0C;EAAnCE,GAAG,EAAC;AAAW,GAAC,aAAW;gEAGpCF,mBAAA,CAA0B;EAArBC,KAAK,EAAC;AAAQ;;EACdA,KAAK,EAAC;AAAmB;iEAC5BD,mBAAA,CAAwC;EAAjCE,GAAG,EAAC;AAAU,GAAC,YAAU;iEAIlCF,mBAAA,CAAmE;EAA3DG,IAAI,EAAC,QAAQ;EAACF,KAAK,EAAC;GAAgB,gBAAc;;EAM3DA,KAAK,EAAC;AAAoB;;EACtBA,KAAK,EAAC;AAAkB;;EACtBA,KAAK,EAAC;AAAoB;;;SA3B1BG,QAAA,CAAAC,IAAI,I,cAAfC,mBAAA,CAoCI,OAAAC,UAAA,GAnCFC,UAEM,EACNR,mBAAA,CA+BM,OA/BNS,UA+BM,GA7BJT,mBAAA,CAgBI,OAhBJU,UAg