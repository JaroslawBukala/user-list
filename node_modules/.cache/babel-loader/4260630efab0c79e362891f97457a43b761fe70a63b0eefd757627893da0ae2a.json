k>\r\n    </div>\r\n\r\n    <div v-for=\"user in filteredUserList\" :key=\"user.id\" class=\"user-item\">\r\n      <div class=\"user-avatar\">\r\n        <img :src=\"user.avatar\" alt=\"User Avatar\">\r\n      </div>\r\n      <div class=\"user-details\">\r\n        <div class=\"user-name\">\r\n          <span>{{ user.first_name }}</span>\r\n          <span>{{ user.last_name }}</span>\r\n        </div>\r\n\r\n        <div class=\"user-actions\">\r\n          <router-link :to=\"{ name: 'edit-user', params: { id: filteredUserList.indexOf(user) +1 } }\">Edit</router-link>\r\n          <button @click=\"deleteUser(user.id)\">Delete</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div>\r\n      <v-pagination\r\n        v-model=\"currentPage\"\r\n        :length=\"totalPages\"\r\n        @input=\"handlePageChange\"\r\n      ></v-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      currentPage: 1,\r\n      itemsPerPage: 10,\r\n    };\r\n  },\r\n  computed: {\r\n    usersNumer() {\r\n      console.log(this.$store.state.filteredUserList.length + 'length of a user list')\r\n      return this.$store.state.userList.length + 1;\r\n    },\r\n    filteredUserList() {\r\n      console.log(this.$store.state.filteredUserList + 'USERS INSIDE HELLO')\r\n      return this.$store.state.filteredUserList;\r\n    },\r\n    totalPages() {\r\n      return Math.ceil(this.filteredUserList.length / this.itemsPerPage);\r\n    },\r\n    paginatedUserList() {\r\n      const startIndex = (this.currentPage - 1) * this.itemsPerPage;\r\n      const endIndex = startIndex + this.itemsPerPage;\r\n      return this.filteredUserList.slice(startIndex, endIndex);\r\n    },\r\n  },\r\n  mounted() {\r\n    if (this.$store.state.userList.length === 0) {\r\n      this.$store.dispatch('fetchUserList');\r\n    }\r\n  },\r\n  methods: {\r\n    searchUsers() {\r\n      this.$store.dispatch('filterUserList', this.searchQuery);\r\n    },\r\n    deleteUser(userId) {\r\n      console.log(userId + 'THE PROBLEM');\r\n      this.$store.dispatch('deleteUser', userId);\r\n    },\r\n    handlePageChange(page) {\r\n      this.currentPage = page;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.search-container {\r\n  display: flex;\r\n  align-items: center;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.search-container input[type=\"text\"] {\r\n  margin-right: 10px;\r\n  padding: 5px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n}\r\n\r\n.add-user-btn {\r\n  background-color: green;\r\n  color: white;\r\n  border: none;\r\n  padding: 5px 10px;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n.user-list {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.user-item {\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 10px;\r\n  border-bottom: 1px solid #ccc;\r\n}\r\n\r\n.user-avatar img {\r\n  width: 50px;\r\n  height: 50px;\r\n  border-radius: 50%;\r\n  object-fit: cover;\r\n  margin-right: 10px;\r\n}\r\n\r\n.user-name {\r\n  font-weight: bold;\r\n}\r\n\r\n.user-actions button {\r\n  margin-left: 10px;\r\n  padding: 5px 10px;\r\n  background-color: red;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n/* Custom styles for v-pagination */\r\n.v-pagination {\r\n  display: flex;\r\n  list-style-type: none;\r\n  padding: 0;\r\n  margin: 0;\r\n}\r\n\r\n.v-pagination li {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  width: 30px; /* Adjust the width as needed to control the size of each rectangle */\r\n  height: 20px; /* Adjust the height as needed to control the size of each rectangle */\r\n  margin-right: 2px; /* Adjust the margin as needed to control the spacing between rectangles */\r\n  background-color: #ccc; /* Set the background color of the rectangles */\r\n  border: none; /* Remove any border */\r\n  cursor: pointer;\r\n}\r\n\r\n.v-pagination li