e clearSearchQuery action\n        if (this.$store.state.userList.length === 0) {\n          return;\n        }\n        this.$router.push({\n          name: 'home'\n        });\n      }).catch(error => {\n        console.error('Failed to delete user:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","mapGetters","data","avatar","avatarPreview","computed","user","console","log","$store","state","filteredUserList","$route","params","id","methods","saveChanges","updatedUser","first_name","last_name","dispatch","$router","push","name","error","openFileDialog","$refs","fileInput","click","handleFileChange","event","file","target","files","URL","createObjectURL","avatarImage","require","deleteUser","then","userList","length","catch"],"sources":["C:\\Users\\User\\Desktop\\Client\\vue-starter-project\\src\\components\\Edit.vue"],"sourcesContent":["<template>\r\n    <div v-if=\"user\">\r\n\r\n  <v-container>\r\n    <div class=\"title-layout\">\r\n      <h2 class=\"add-user-title\">Edit User</h2>\r\n    </div>\r\n    <div class=\"main-container\">\r\n      <v-container class=\"add-user-section\">\r\n        <form @submit.prevent=\"addUser\">\r\n          <div class=\"flex\">\r\n            <div class=\"form-field-inline\">\r\n              <label for=\"firstName\">First Name:</label>\r\n              <input type=\"text\" v-model=\"user.first_name\" required>\r\n            </div>\r\n            <div class=\"form-field-inline\">\r\n              <label for=\"lastName\">Last Name:</label>\r\n              <input type=\"text\" v-model=\"user.last_name\" required>\r\n            </div>\r\n          </div>\r\n          <button type=\"submit\" class=\"update-button\" @click=\"saveChanges\">Update Details</button>\r\n        <button class=\"delete-button\" @click=\"deleteUser\">Delete User</button>        \r\n      </form>\r\n      </v-container>\r\n      <div class=\"margin\"></div>\r\n      <v-container class=\"add-avatar-section\">\r\n        <div class=\