{"ast":null,"code":"export default {\n  data() {\n    return {\n      searchQuery: '',\n      currentPage: 1,\n      itemsPerPage: 11,\n      // Number of items to display per page\n      paginationRange: 11 // Number of pagination numbers to display\n    };\n  },\n\n  computed: {\n    usersNumer() {\n      return this.$store.state.filteredUserList.length + 1;\n    },\n    filteredUserList() {\n      return this.$store.state.filteredUserList.map(user => ({\n        ...user\n      }));\n    },\n    totalPages() {\n      const totalUsers = this.filteredUserList.length;\n      return Math.ceil(totalUsers / this.itemsPerPage);\n    },\n    paginatedUserList() {\n      const startIndex = (this.currentPage - 1) * this.itemsPerPage;\n      const endIndex = startIndex + this.itemsPerPage;\n      return this.filteredUserList.slice(startIndex, endIndex);\n    },\n    visiblePages() {\n      const total = this.totalPages;\n      const current = this.currentPage;\n      const range = this.paginationRange;\n      if (total <= range) {\n        return Array.from({\n          length: total\n        }, (_, i) => i + 1);\n      }\n      const halfRange = Math.floor(range / 2);\n      let startPage = current - halfRange;\n      let endPage = current + halfRange;\n      if (startPage <= 0) {\n        startPage = 1;\n        endPage = range;\n      }\n      if (endPage > total) {\n        endPage = total;\n        startPage = total - range + 1;\n      }\n      return Array.from({\n        length: range\n      }, (_, i) => i + startPage);\n    }\n  },\n  mounted() {\n    if (this.$store.state.userList.length === 0) {\n      this.$store.dispatch('fetchUserList');\n    }\n  },\n  methods: {\n    searchUsers() {\n      this.$store.commit('filterUserList', this.searchQuery);\n    },\n    deleteUser(userId) {\n      this.$store.dispatch('deleteUser', userId);\n    }\n  }\n};","map":{"version":3,"names":["data","searchQuery","currentPage","itemsPerPage","paginationRange","computed","usersNumer","$store","state","filteredUserList","length","map","user","totalPages","totalUsers","Math","ceil","paginatedUserList","startIndex","endIndex","slice","visiblePages","total","current","range","Array","from","_","i","halfRange","floor","startPage","endPage","mounted","userList","dispatch","methods","searchUsers","commit","deleteUser","userId"],"sources":["C:\\Users\\User\\Desktop\\Client\\vue-starter-project\\src\\components\\Hello.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"search-container\">\r\n      <input type=\"text\" placeholder=\"Search...\" v-model=\"searchQuery\" @input=\"searchUsers\" />\r\n      <router-link :to=\"{ name: 'add-user', params: { id: usersNumer }}\">Add user</router-link>\r\n    </div>\r\n    <ul>\r\n      <li v-for=\"user in paginatedUserList\" :key=\"user.id\">\r\n        <div class=\"user-item\">\r\n          <div class=\"user-avatar\">\r\n            <img :src=\"user.avatar\" alt=\"User Avatar\">\r\n          </div>\r\n          <div class=\"user-details\">\r\n            <div class=\"user-name\">\r\n              <span>{{ user.first_name }}</span>\r\n              <span>{{ user.last_name }}</span>\r\n            </div>\r\n            <div class=\"user-actions\">\r\n              <router-link :to=\"{ name: 'edit-user', params: { id: user.id } }\">Edit</router-link>\r\n              <button @click=\"deleteUser(user.id)\">Delete</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </li>\r\n    </ul>\r\n    <div class=\"pagination\">\r\n      <span class=\"pagination-item\" v-for=\"page in visiblePages\" :key=\"page\" :class=\"{ 'active': page === currentPage }\">\r\n        <span class=\"pagination-number\">{{ page }}</span>\r\n        <span class=\"pagination-markings\"></span>\r\n      </span>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      currentPage: 1,\r\n      itemsPerPage: 11, // Number of items to display per page\r\n      paginationRange: 11, // Number of pagination numbers to display\r\n    };\r\n  },\r\n  computed: {\r\n    usersNumer() {\r\n      return this.$store.state.filteredUserList.length + 1;\r\n    },\r\n    filteredUserList() {\r\n      return this.$store.state.filteredUserList.map((user) => ({ ...user }));\r\n    },\r\n    totalPages() {\r\n      const totalUsers = this.filteredUserList.length;\r\n      return Math.ceil(totalUsers / this.itemsPerPage);\r\n    },\r\n    paginatedUserList() {\r\n      const startIndex = (this.currentPage - 1) * this.itemsPerPage;\r\n      const endIndex = startIndex + this.itemsPerPage;\r\n      return this.filteredUserList.slice(startIndex, endIndex);\r\n    },\r\n    visiblePages() {\r\n      const total = this.totalPages;\r\n      const current = this.currentPage;\r\n      const range = this.paginationRange;\r\n\r\n      if (total <= range) {\r\n        return Array.from({ length: total }, (_, i) => i + 1);\r\n      }\r\n\r\n      const halfRange = Math.floor(range / 2);\r\n      let startPage = current - halfRange;\r\n      let endPage = current + halfRange;\r\n\r\n      if (startPage <= 0) {\r\n        startPage = 1;\r\n        endPage = range;\r\n      }\r\n\r\n      if (endPage > total) {\r\n        endPage = total;\r\n        startPage = total - range + 1;\r\n      }\r\n\r\n      return Array.from({ length: range }, (_, i) => i + startPage);\r\n    },\r\n  },\r\n  mounted() {\r\n    if (this.$store.state.userList.length === 0) {\r\n      this.$store.dispatch('fetchUserList');\r\n    }\r\n  },\r\n  methods: {\r\n    searchUsers() {\r\n      this.$store.commit('filterUserList', this.searchQuery);\r\n    },\r\n    deleteUser(userId) {\r\n      this.$store.dispatch('deleteUser', userId);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/* Your existing styles */\r\n</style>\r\n\r\n\r\n<style>\r\n.search-container {\r\n  display: flex;\r\n  align-items: center;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.search-container input[type=\"text\"] {\r\n  margin-right: 10px;\r\n  padding: 5px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n}\r\n\r\n.add-user-btn {\r\n  background-color: green;\r\n  color: white;\r\n  border: none;\r\n  padding: 5px 10px;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n.user-list {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.user-item {\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 10px;\r\n  border-bottom: 1px solid #ccc;\r\n}\r\n\r\n.user-avatar img {\r\n  width: 50px;\r\n  height: 50px;\r\n  border-radius: 50%;\r\n  object-fit: cover;\r\n  margin-right: 10px;\r\n}\r\n\r\n.user-name {\r\n  font-weight: bold;\r\n}\r\n\r\n.user-actions button {\r\n  margin-left: 10px;\r\n  padding: 5px 10px;\r\n  background-color: red;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n.pagination {\r\n  margin-top: 20px;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.pagination-item {\r\n  display: flex;\r\n  align-items: center;\r\n  margin-right: 5px;\r\n}\r\n\r\n.pagination-number {\r\n  width: 5mm;\r\n  height: 7mm;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background-color: #ccc;\r\n  color: #fff;\r\n}\r\n\r\n.pagination-item.active .pagination-number {\r\n  background-color: #333;\r\n}\r\n\r\n.pagination-markings {\r\n  width: 0;\r\n  height: 0;\r\n  border-top: 5px solid transparent;\r\n  border-bottom: 5px solid transparent;\r\n  border-left: 5px solid #333;\r\n  margin-left: 2px;\r\n  margin-right: 2px;\r\n}\r\n</style>\r\n"],"mappings":"AAmCA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,WAAW,EAAE,CAAC;MACdC,YAAY,EAAE,EAAE;MAAE;MAClBC,eAAe,EAAE,EAAE,CAAE;IACvB,CAAC;EACH,CAAC;;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,gBAAgB,CAACC,MAAK,GAAI,CAAC;IACtD,CAAC;IACDD,gBAAgBA,CAAA,EAAG;MACjB,OAAO,IAAI,CAACF,MAAM,CAACC,KAAK,CAACC,gBAAgB,CAACE,