{"ast":null,"code":"import { computed, onMounted, ref } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  setup() {\n    const store = useStore();\n    const selectedUser = ref(null);\n    const showEditModal = ref(false);\n    const filteredUserList = computed(() => {\n      // Implement your filtering logic using store.state.userList\n      // You can access the user list as store.state.userList in computed properties and methods\n      return store.state.userList;\n    });\n    const fetchUserList = () => {\n      if (store.state.userList.length === 0) {\n        store.dispatch('fetchUserList');\n      }\n    };\n    const editUser = user => {\n      selectedUser.value = user;\n      showEditModal.value = true;\n    };\n    const handleUserUpdated = updatedUser => {\n      store.dispatch('updateUser', updatedUser);\n      showEditModal.value = false;\n    };\n    const handleUserDeleted = userId => {\n      store.dispatch('deleteUser', userId);\n      showEditModal.value = false;\n    };\n    onMounted(fetchUserList);\n    return {\n      filteredUserList,\n      selectedUser,\n      showEditModal,\n      editUser,\n      handleUserUpdated,\n      handleUserDeleted\n    };\n  }\n};","map":{"version":3,"names":["computed","onMounted","ref","useStore","setup","store","selectedUser","showEditModal","filteredUserList","state","userList","fetchUserList","length","dispatch","editUser","user","value","handleUserUpdated","updatedUser","handleUserDeleted","userId"],"sources":["C:\\Users\\User\\Desktop\\Client\\vue-starter-project\\src\\components\\Hello.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>User List</h1>\r\n    <ul>\r\n      <li v-for=\"user in filteredUserList\" :key=\"user.id\">\r\n        {{ user.fullName }}\r\n        <button @click=\"editUser(user)\">Edit</button>\r\n      </li>\r\n    </ul>\r\n\r\n    <div v-if=\"showEditModal\">\r\n      <edit-user\r\n        :user=\"selectedUser\"\r\n        @user-updated=\"handleUserUpdated\"\r\n        @user-deleted=\"handleUserDeleted\"\r\n      />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed, onMounted, ref } from 'vue';\r\nimport { useStore } from 'vuex';\r\n\r\nexport default {\r\n  setup() {\r\n    const store = useStore();\r\n    const selectedUser = ref(null);\r\n    const showEditModal = ref(false);\r\n\r\n    const filteredUserList = computed(() => {\r\n      // Implement your filtering logic using store.state.userList\r\n      // You can access the user list as store.state.userList in computed properties and methods\r\n      return store.state.userList;\r\n    });\r\n\r\n    const fetchUserList = () => {\r\n      if (store.state.userList.length === 0) {\r\n        store.dispatch('fetchUserList');\r\n      }\r\n    };\r\n\r\n    const editUser = (user) => {\r\n      selectedUser.value = user;\r\n      showEditModal.value = true;\r\n    };\r\n\r\n    const handleUserUpdated = (updatedUser) => {\r\n      store.dispatch('updateUser', updatedUser);\r\n      showEditModal.value = false;\r\n    };\r\n\r\n    const handleUserDeleted = (userId) => {\r\n      store.dispatch('deleteUser', userId);\r\n      showEditModal.value = false;\r\n    };\r\n\r\n    onMounted(fetchUserList);\r\n\r\n    return {\r\n      filteredUserList,\r\n      selectedUser,\r\n      showEditModal,\r\n      editUser,\r\n      handleUserUpdated,\r\n      handleUserDeleted,\r\n    };\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAqBA,SAASA,QAAQ,EAAEC,SAAS,EAAEC,GAAE,QAAS,KAAK;AAC9C,SAASC,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIF,QAAQ,CAAC,CAAC;IACxB,MAAMG,YAAW,GAAIJ,GAAG,CAAC,IAAI,CAAC;IAC9B,MAAMK,aAAY,GAAIL,GAAG,CAAC,KAAK,CAAC;IAEhC,MAAMM,gBAAe,GAAIR,QAAQ,CAAC,MAAM;MACtC;MACA;MACA,OAAOK,KAAK,CAACI,KAAK,CAACC,QAAQ;IAC7B,CAAC,CAAC;IAEF,MAAMC,aAAY,GAAIA,CAAA,KAAM;MAC1B,IAAIN,KAAK,CAACI,KAAK,CAACC,QAAQ,CAACE,MAAK,KAAM,CAAC,EAAE;QACrCP,KAAK,CAACQ,QAAQ,CAAC,eAAe,CAAC;MACjC;IACF,CAAC;IAED,MAAMC,QAAO,GAAKC,IAAI,IAAK;MACzBT,YAAY