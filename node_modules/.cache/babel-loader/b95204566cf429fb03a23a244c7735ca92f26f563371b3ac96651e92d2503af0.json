{"ast":null,"code":"import axios from 'axios';\nimport EditUser from '@/components/Edit.vue';\nexport default {\n  data() {\n    return {\n      searchUser: '',\n      interns: [],\n      filteredInterns: [],\n      userList: [],\n      showEditModal: false,\n      selectedUserId: null\n    };\n  },\n  mounted() {\n    console.log('Parent component mounted');\n    this.fetchUserList();\n  },\n  computed: {\n    selectedUser() {\n      return this.userList.find(user => user.id === this.selectedUserId);\n    }\n  },\n  methods: {\n    async search() {\n      try {\n        if (this.searchUser.trim() === '') {\n          this.filteredInterns = [];\n          return;\n        }\n        const response = await axios.get('https://reqres.in/api/users', {\n          params: {\n            q: this.searchUser\n          }\n        });\n        this.interns = response.data.data;\n        this.filterInterns();\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    filterInterns() {\n      this.filteredInterns = this.interns.filter(intern => {\n        const fullName = `${intern.first_name} ${intern.last_name}`.toLowerCase();\n        return fullName.includes(this.searchUser.toLowerCase()) && this.userList.some(user => user.id === intern.id);\n      });\n    },\n    fetchUserList() {\n      axios.get('https://reqres.in/api/users').then(response => {\n        this.userList = response.data.data.map(user => ({\n          id: user.id,\n          fullName: `${user.first_name} ${user.last_name}`,\n          avatar: user.avatar\n        }));\n      }).catch(error => {\n        console.error('Failed to fetch user list:', error);\n      });\n    },\n    handleUserUpdated(updatedUser) {\n      const userIndex = this.userList.findIndex(user => user.id === updatedUser.id);\n      if (userIndex !== -1) {\n        this.$set(this.userList, userIndex, updatedUser);\n        if (this.selectedUserId === updatedUser.id) {\n          this.selectedUserId = null; // Reset the selected user\n        }\n      }\n    },\n\n    handleUserDeleted(userId) {\n      const userIndex = this.userList.findIndex(user => user.id === userId);\n      if (userIndex !== -1) {\n        this.userList.splice(userIndex, 1);\n      }\n    }\n  },\n  components: {\n    EditUser\n  }\n};","map":{"version":3,"names":["axios","EditUser","data","searchUser","interns","filteredInterns","userList","showEditModal","selectedUserId","mounted","console","log","fetchUserList","computed","selectedUser","find","user","id","methods","search","trim","response","get","params","q","filterInterns","error","filter","intern","fullName","first_name","last_name","toLowerCase","includes","some","then","map","avatar","catch","handleUserUpdated","updatedUser","userIndex","findIndex","$set","handleUserDeleted","userId","splice","components"],"sources":["C:\\Users\\User\\Desktop\\Client\\vue-starter-project\\src\\components\\Hello.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <div class=\"search-container\">\r\n        <input type=\"text\" placeholder=\"Search...\" v-model=\"searchUser\" @input=\"search\" />\r\n        <button class=\"add-user-btn\">Add User</button>\r\n      </div>\r\n      <ul>\r\n        <li v-for=\"intern in filteredInterns\" :key=\"intern.id\">\r\n          {{ intern.first_name }} {{ intern.last_name }}\r\n        </li>\r\n      </ul>\r\n  \r\n      <div class=\"user-list\">\r\n        <div v-for=\"user in userList\" :key=\"user.id\" class=\"user-item\">\r\n          <div class=\"user-avatar\">\r\n            <img :src=\"user.avatar\" alt=\"User Avatar\">\r\n          </div>\r\n          <div class=\"user-details\">\r\n            <div class=\"user-name\">{{ user.fullName }}</div>\r\n            <div class=\"user-actions\">\r\n              <router-link :to=\"{ name: 'edit-user', params: { id: user.id } }\">Edit</router-link>\r\n              <button @click=\"handleUserDeleted(user.id)\">Delete</button>\r\n            </div>\r\n            <div v-if=\"showEditModal && user.id === selectedUserId\">\r\n                <edit-user :user=\"selectedUser\" @user-updated=\"handleUserUpdated\" @user-deleted=\"handleUserDeleted\" />\r\n\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  import EditUser from '@/components/Edit.vue';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        searchUser: '',\r\n        interns: [],\r\n        filteredInterns: [],\r\n        userList: [],\r\n        showEditModal: false,\r\n        selectedUserId: null\r\n      };\r\n    },\r\n  \r\n    mounted() {\r\n      console.log('Parent component mounted');\r\n      this.fetchUserList();\r\n    },\r\n    computed: {\r\n  selectedUser() {\r\n    return this.userList.find((user) => user.id === this.selectedUserId);\r\n  },\r\n},\r\n\r\n  \r\n    methods: {\r\n      async search() {\r\n        try {\r\n          if (this.searchUser.trim() === '') {\r\n            this.filteredInterns = [];\r\n            return;\r\n          }\r\n  \r\n          const response = await axios.get('https://reqres.in/api/users', {\r\n            params: {\r\n              q: this.searchUser,\r\n            },\r\n          });\r\n  \r\n          this.interns = response.data.data;\r\n          this.filterInterns();\r\n        } catch (error) {\r\n          console.error(error);\r\n        }\r\n      },\r\n  \r\n      filterInterns() {\r\n        this.filteredInterns = this.interns.filter((intern) => {\r\n          const fullName = `${intern.first_name} ${intern.last_name}`.toLowerCase();\r\n          return (\r\n            fullName.includes(this.searchUser.toLowerCase()) &&\r\n            this.userList.some((user) => user.id === intern.id)\r\n          );\r\n        });\r\n      },\r\n  \r\n      fetchUserList() {\r\n        axios\r\n          .get('https://reqres.in/api/users')\r\n          .then((response) => {\r\n            this.userList = response.data.data.map((user) => ({\r\n              id: user.id,\r\n              fullName: `${user.first_name} ${user.last_name}`,\r\n              avatar: user.avatar,\r\n            }));\r\n          })\r\n          .catch((error) => {\r\n            console.error('Failed to fetch user list:', error);\r\n          });\r\n      },\r\n  \r\n      handleUserUpdated(updatedUser) {\r\n  const userIndex = this.userList.findIndex((user) => user.id === updatedUser.id);\r\n  if (userIndex !== -1) {\r\n    this.$set(this.userList, userIndex, updatedUser);\r\n    if (this.selectedUserId === updatedUser.id) {\r\n      this.selectedUserId = null; // Reset the selected user\r\n    }\r\n  }\r\n},\r\n\r\n\r\n  \r\n      handleUserDeleted(userId) {\r\n        const userIndex = this.userList.findIndex((user) => user.id === userId);\r\n        if (userIndex !== -1) {\r\n          this.userList.splice(userIndex, 1);\r\n        }\r\n      },\r\n    },\r\n  \r\n    components: {\r\n      EditUser,\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  .search-container {\r\n    display: flex;\r\n    align-items: center;\r\n  }\r\n  \r\n  input[type=\"text\"] {\r\n    margin-right: 10px;\r\n  }\r\n  \r\n  .add-user-btn {\r\n    background-color: green;\r\n    color: white;\r\n    border: none;\r\n    padding: 5px 10px;\r\n  }\r\n  \r\n  .user-list {\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n  \r\n  .user-item {\r\n    display: flex;\r\n    align-items: center;\r\n    padding: 10px;\r\n    border-bottom: 1px solid #ccc;\r\n  }\r\n  \r\n  .user-avatar img {\r\n    width: 50px;\r\n    height: 50px;\r\n    border-radius: 50%;\r\n    object-fit: cover;\r\n    margin-right: 10px;\r\n  }\r\n  \r\n  .user-name {\r\n    font-weight: bold;\r\n  }\r\n  \r\n  .user-actions button {\r\n    margin-left: 10px;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAkCE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,uBAAuB;AAE5C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MAC